{
  "name": "app-base",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json -o ",
    "build": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/app-base/stats.json",
    "test": "ng test --code-coverage",
    "test:mutation": "stryker run && node utilidades/mutation/generate-mutation-sonar-report.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:run": "ng run app-base:cypress-run --config-file ./cypress.e2e.config.ts --browser chrome",
    "bdd:run": "ng run app-base:cypress-run --config-file ./cypress.bdd.config.ts --browser chrome",
    "bdd": "ng e2e --config-file ./cypress.bdd.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/localize": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "ngx-cookie-service": "^14.0.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@badeball/cypress-cucumber-preprocessor": "^11.5.1",
    "@cypress/schematic": "^2.0.0",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/karma-runner": "^6.0.2",
    "@stryker-mutator/typescript-checker": "^6.0.2",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cypress": "^10.3.0",
    "eslint": "^8.23.0",
    "eslint-plugin-rule-angular-arquitecture": "file:utilidades/eslint-plugin-rule-angular-arquitecture/",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "license-checker": "^25.0.1",
    "patch-package": "^6.4.7",
    "stryker-cli": "^1.0.2",
    "ts-loader": "latest",
    "ts-node": "~ 10.8.0",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
